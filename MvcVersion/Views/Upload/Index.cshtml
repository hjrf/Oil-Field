
@{
    ViewBag.Title = "Index";
}

    <!-- DROPZONE -->
    <link rel="stylesheet" type="text/css" href="~/Scripts/templete/js/dropzone/dropzone.min.css" />
    <!-- FONTS -->
    <link href='http://fonts.useso.com/css?family=Open+Sans:300,400,600,700' rel='stylesheet' type='text/css'>

    <!-- /PAGE HEADER -->
    <!-- DROPZONE -->
    <div class="row">
        <div class="col-md-12">
            <!-- BOX -->
            <div class="box border blue">
                <div class="box-title">
                    <h4><i class="fa fa-cloud-download"></i>文件上传（当前支持xls格式，且保证第一行为标题）</h4>
                    <div class="tools hidden-xs">
                        <a href="#box-config" data-toggle="modal" class="config">
                            <i class="fa fa-cog"></i>
                        </a>
                        <a href="javascript:;" class="reload">
                            <i class="fa fa-refresh"></i>
                        </a>
                        <a href="javascript:;" class="collapse">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a href="javascript:;" class="remove">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="box-body">
                    <form action="#"
                          class="dropzone"
                          id="my-awesome-dropzone">
                        <div class="fallback">
                            <input name="file" type="file" multiple="" />
                        </div>
                    </form>
                </div>
            </div>
            <!-- /BOX -->
        </div>
    </div>
    <!-- /DROPZONE -->
    <div class="footer-tools">
        <span class="go-top">
            <i class="fa fa-chevron-up"></i> Top
        </span>
    </div>
    <!--/PAGE -->
    <!-- JAVASCRIPTS -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- JQUERY -->
    <script src="~/Scripts/templete/js/jquery/jquery-2.0.3.min.js"></script>
    <!-- JQUERY UI-->
    <script src="~/Scripts/templete/js/jquery-ui-1.10.3.custom/js/jquery-ui-1.10.3.custom.min.js"></script>
    <!-- BOOTSTRAP -->
    <script src="~/Scripts/templete/bootstrap-dist/js/bootstrap.min.js"></script>


    <!-- DATE RANGE PICKER -->
    <script src="~/Scripts/templete/js/bootstrap-daterangepicker/moment.min.js"></script>

    <script src="~/Scripts/templete/js/bootstrap-daterangepicker/daterangepicker.min.js"></script>
    <!-- SLIMSCROLL -->
    <script type="text/javascript" src="~/Scripts/templete/js/jQuery-slimScroll-1.3.0/jquery.slimscroll.min.js"></script>
    <script type="text/javascript" src="~/Scripts/templete/js/jQuery-slimScroll-1.3.0/slimScrollHorizontal.min.js"></script>
    <!-- BLOCK UI -->
    <script type="text/javascript" src="~/Scripts/templete/js/jQuery-BlockUI/jquery.blockUI.min.js"></script>
    <!-- DROPZONE -->
    <script type="text/javascript" src="~/Scripts/templete/js/dropzone/dropzone.min.js"></script>
    <!-- COOKIE -->
    <script type="text/javascript" src="~/Scripts/templete/js/jQuery-Cookie/jquery.cookie.min.js"></script>
    <!-- CUSTOM SCRIPT -->
    <script src="~/Scripts/templete/js/script.js"></script>
    <script>
		jQuery(document).ready(function() {
			App.setPage("dropzone_file_upload");  //Set current page
			App.init(); //Initialise plugins and elements
		});
    </script>
    <!-- /JAVASCRIPTS -->
</body>
</html>

